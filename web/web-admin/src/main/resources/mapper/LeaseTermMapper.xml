<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lease.webadmin.mapper.LeaseTermMapper">

    <resultMap id="BaseResultMap" type="com.lease.model.entity.LeaseTerm">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="monthCount" column="month_count" jdbcType="INTEGER"/>
            <result property="unit" column="unit" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,month_count,unit,
        create_time,update_time,is_deleted
    </sql>
    <select id="selectListByRoomId" resultType="com.lease.model.entity.LeaseTerm">
        select id,
               month_count,
               unit
        from lease_term
        where is_deleted = 0
          and id in (select lease_term_id
                     from room_lease_term
                     where is_deleted = 0
                       and room_id = #{id})
    </select>
</mapper>
